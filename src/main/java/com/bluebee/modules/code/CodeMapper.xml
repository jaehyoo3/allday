<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bluebee.modules.code.CodeMapper">

    <resultMap id="resultMapObj" type="com.bluebee.modules.code.Code"></resultMap>
	
	<select id="selectList" resultMap="resultMapObj">
		SELECT
			a.ccgSeq
		    ,a.codeName
		    ,b.ccSeq
		    ,b.cname
		    ,b.yn
		    FROM codegroup a
		    LEFT JOIN code b ON b.codegroup_ccgSeq = a.ccgSeq
				
	</select>
</mapper>	